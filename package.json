    "ejs": "^3.1.6",
    "email-verification-code": "^2.0.4",
    "express": "^4.17.3",
    "express-validator": "^6.14.0",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.3",
